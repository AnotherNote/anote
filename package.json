{
  "name": "Anote",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "productName": "Anote",
  "main": "index.js",
  "scripts": {
    "build_js": "babel --quiet src --out-dir build",
    "build": "babel --quiet src --out-dir build && electron-packager . Anote --platform=darwin --arch=x64 --ignore=node_modules/electron-*",
    "start": "npm run build_js && electron .",
    "develop": "babel src --out-dir build --watch & electron .",
    "package": "asar pack Anote-darwin-x64/Anote.app/Contents/Resources/app Anote-darwin-x64/Anote.app/Contents/Resources/app.asar --unpack-dir '{db}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpzero/electron-makedown.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {},
  "homepage": "https://github.com/wpzero/electron-makedown.git#readme",
  "devDependencies": {
    "asar": "^0.12.1",
    "babel-cli": "^6.11.4",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "electron-rebuild": "^1.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.1",
    "co": "^4.6.0",
    "electron-prebuilt": "^1.2.0",
    "jquery": "^3.1.0",
    "keymirror": "^0.1.1",
    "material-ui": "^0.15.4",
    "nedb": "^1.8.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-dropzone": "^3.5.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "sanitize-html": "^1.13.0",
    "to-markdown": "^3.0.1"
  }
}
